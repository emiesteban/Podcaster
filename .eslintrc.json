{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": ["plugin:react/jsx-runtime", "standard-with-typescript", "plugin:jest/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": ["./tsconfig.json"]
  },
  "plugins": ["react","jest"],
  "rules": {
    "eqeqeq": "error",
    "no-unused-vars": "error",
    "linebreak-style": ["error", "unix"],
    "quotes": ["error", "single"],
    "semi": "off",
    "prettier/prettier": "warn",
    "@typescript-eslint/prefer-nullish-coalescing": "warn"
  }
}
